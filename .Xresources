! Transparency (0-1):
st.alpha: 0.85
! alphaOffset is the alpha offset between focused and unfocused windows
st.alphaOffset: 0

!! Set a default font and font size as below:
st.font: MesloLGS NF:size=13:antialias=true:autohint=true;

! st.termname: st-256color
! st.borderpx: 2

!! Set dmenu resources:

!! material palenight:
dmenu.font :  MesloLGS NF:size=13:antialias=true:autohint=true;
dmenu.background : #292d3e
dmenu.foreground : #82aaff
dmenu.selbackground : #82aaff
dmenu.selforeground : #292d3e

!! catppuccin :D :
